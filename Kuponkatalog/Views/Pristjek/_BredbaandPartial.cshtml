tion('months')}} {{>~printInterval(totalPriceForFirst6months, null, ~getTranslation('currency'), true, true, true)}}</div></div></div><div class="row-item margin-top"><div class="col-xs-12 col-sm-9 col-md-8 col-md-offset-2 col-lg-8 col-lg-offset-2"><div class="col-md-12 as-small text-center-xs">{{if ~isValid(Comment)}} {{>Comment}} {{/if}}</div></div><div class="col-xs-12 col-sm-3 col-md-2 col-lg-2 text-center small-padding-xs margin-top-xs"><div class="as-read-more nowrap no-margin light-hover" style="position:static"><a class="as-small move-up-3px" data-open="{{>~getTranslation('showMore')}}" data-close="{{>~getTranslation('showLess')}}">{{>~getTranslation('showMore')}}</a><div class="as-read-more-icon margin-left-xs"><i class="fa fa-angle-down fa-lg"></i></div></div></div></div><div class="row-item as-show-more-content row-padding"><div class="hr-divider"></div><div class="col-xs-12 col-md-push-2 col-md-8 text-center-sm text-center-xs">{{if ~isValid(additionalFees)}}<div class="more-col col-xs-6 col-sm-4 col-md-3"><div class="as-data-type">{{>~getTranslation('additionalFees')}}</div><div class="as-data-c">{{>~printInterval(additionalFees, null, ~getTranslation('currency'), true, true, true)}}</div></div>{{/if}}<div class="more-col col-xs-6 col-sm-4 col-md-3"><div class="as-data-type">{{>~getTranslation('minContractPeriod')}}</div><div class="as-data-c">{{if minContractPeriodInMonths == 1}} {{>~printInterval(minContractPeriodInMonths, null, ~getTranslation('month'))}} {{else}} {{>~printInterval(minContractPeriodInMonths, null, ~getTranslation('months'))}} {{/if}}</div></div><div class="more-col col-xs-6 col-sm-4 col-md-3"><div class="as-data-type">{{>~getTranslation('minimumPrice')}} 6 {{>~getTranslation('months')}}</div><div class="as-data-c">{{>~printInterval(totalPriceForFirst6months, null, ~getTranslation('currency'), true, true, true)}}</div></div><div class="more-col col-xs-6 col-sm-4 col-md-3"><div class="as-data-type">{{>~getTranslation('possibilityForStaticIP')}}</div><div class="as-data-c">{{>~printOrGetBoolean(possibilityForStaticIP)}}</div></div>{{if ~isValid(minMonthlyTransactionFee)}}<div class="more-col col-xs-6 col-sm-4 col-md-3"><div class="as-data-type">{{>~getTranslation('minTransactionFee')}}</div><div class="as-data-c">{{>~printInterval(minMonthlyTransactionFee, null, ~getTranslation('currency'), true, true, true)}}</div></div>{{/if}}<div class="more-col col-xs-6 col-sm-4 col-md-3"><div class="as-data-type">{{>~getTranslation('includesWirelessRouter')}}</div><div class="as-data-c">{{>~printOrGetBoolean(includingWirelessRouter)}}</div></div>{{if ~printOrGetBoolean(connectionViaTVCable, true) || ~printOrGetBoolean(connectionViaLandLinePhone, true)}}<div class="more-col col-xs-6 col-sm-4 col-md-3"><div class="as-data-type">{{>~getTranslation('connectedThrough')}}</div><div class="as-data-c">{{if ~printOrGetBoolean(connectionViaTVCable, true)}} {{>~getTranslation('tvCable')}}<br>{{/if}} {{if ~printOrGetBoolean(connectionViaLandLinePhone, true)}} {{>~getTranslation('landlinePhone')}} {{/if}}</div></div>{{/if}} {{if ~isValid(extraServicesIncluded)}}<div class="more-col col-xs-6 col-sm-4 col-md-3"><div class="as-data-type">{{>~getTranslation('alsoIncludes')}}</div><div class="as-data-c">{{>extraServicesIncluded}}</div></div>{{/if}} {{if ~isValid(specialDemands)}}<div class="more-col col-xs-12 col-sm-12 col-md-12"><div class="as-data-type">{{>~getTranslation('specialDemands')}}</div><div class="as-data-c">{{>specialDemands}}</div></div>{{/if}}</div></div></div>

